ws() {\n      const bandId = this.$store.state.bandId;\n      getBandAlbumReviews(bandId).then((res) => {\n        this.tableData = res.data;\n        this.pagiDataAll = Math.ceil(res.data.length / 10); // 假设每页显示10条数据\n      });\n    },\n    // 分页跳转\n    currentChange(item) {\n      this.getMusicReviews(item - 1);\n    },\n    // 刷新乐评表\n    refresh(event) {\n      this.lostBlur(event);\n      this.getMusicReviews();\n    },\n    // 失焦处理\n    lostBlur(event) {\n      let target = event.target;\n      if (target.nodeName == \"SPAN\") {\n        target = event.target.parentNode;\n      }\n      target.blur();\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.batchOption {\n  margin-bottom: 5px;\n  .el-button {\n    font-size: 10px;\n    padding: 5px;\n  }\n}\n.pagination {\n  text-align: center;\n  margin-top: 15px;\n}\n</style>"],"sourceRoot":"src/views/band/band_MusicReview"}]}